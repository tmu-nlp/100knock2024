   "source": [
    "import pandas as pd\n",
    "import re\n",
    "wiki = pd.read_json('Data/jawiki-country.json.gz', lines = True)\n",
    "uk = wiki[wiki['title']=='イギリス'].text.values\n",
    "ls = uk[0].split('\\n')\n",
    "pattern = re.compile('Category')\n",
    "for line in ls:\n",
    "    if re.search(pattern, line):\n",
    "        print (line)"
