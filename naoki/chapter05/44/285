digraph {
	"須藤は" -> "している"
	"哲学的に" -> "論じられている"
	"論じられている" -> "原因という"
	"原因という" -> "言葉を"
	"言葉を" -> "取り上げて"
	"取り上げて" -> "述べており"
	"原因という" -> "言葉を"
	"言葉を" -> "定義しない"
	"具体的に" -> "定義しない"
	"定義しない" -> "限り"
	"限り" -> "不可能ですと"
	"それ以上の" -> "議論は"
	"議論は" -> "不可能ですと"
	"不可能ですと" -> "述べており"
	"述べており" -> "している"
	"哲学者が" -> "持っている"
	"興味を" -> "持っている"
	"持っている" -> "因果の"
	"因果の" -> "定義が"
	"定義が" -> "違う"
	"物理学者とは" -> "違う"
	"違う" -> "ことは"
	"ことは" -> "確かでしょうと"
	"確かでしょうと" -> "している"
}
