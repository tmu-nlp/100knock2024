    "#動詞の基本形をすべて抽出せよ\n",
    "\n",
    "import knock30\n",
    "\n",
    "verbs_base = list()\n",
    "\n",
    "for line in knock30.sentence_list:\n",
    "    for morph in line:\n",
    "        if morph['pos'] == \"動詞\":\n",
    "            verbs_base.append(morph['base'])\n",
    "print(verbs_base)"
   ]
  }
