    "import re\n",
    "def cipher(text):\n",
    "    temp = \"\"\n",
    "    for ch in text:\n",
    "        if ch in [chr(i) for i in range(ord('a'),ord('z')+1)]:\n",
    "            ch = chr(219 - ord(ch))\n",
    "        temp += ch\n",
    "    return temp\n",
    "password = cipher(\"aaabbb\")\n",
    "re_password = cipher(password)\n",
    "\n",
    "password,re_password"
   ]
  }
